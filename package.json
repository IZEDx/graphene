{
  "name": "graphene-cms",
  "scripts": {
    "bootstrap": "npx lerna bootstrap",
    "build": "npm run bootstrap && npx lerna run build",
    "build:api": "cd ./packages/api && npm run build",
    "build:ui": "cd ./packages/ui && npm run build",
    "dev": "npm run bootstrap && (npm run dev:ui & npm run dev:api)",
    "dev:api": "cd ./packages/api && npm run dev",
    "dev:ui": "cd ./packages/ui && npm run dev",
    "test": "npx lerna run test",
    "start": "npm run && node bin/server.js",
    "typesync": "npx typesync"
  },
  "devDependencies": {
    "@nestjs/cli": "^6.14.1",
    "@stencil/core": "latest",
    "@stencil/router": "latest",
    "@stencil/sass": "latest",
    "@types/object-hash": "^1.3.1",
    "bulma": "latest",
    "bulma-dashboard": "^0.3.1",
    "bulma-switch": "^2.0.0",
    "chalk": "latest",
    "change-case": "^4.1.1",
    "cross-env": "latest",
    "graphql": "^14.5.8",
    "graphql-request": "^1.8.2",
    "lerna": "latest",
    "object-hash": "^2.0.1",
    "parcel": "latest",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "stencil-quantum": "alpha",
    "typescript": "latest",
    "typesync": "latest"
  },
  "dependencies": {}
}
